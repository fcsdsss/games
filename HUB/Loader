local function loadScript(scriptName, scriptUrl)
    Luna:Notification({ Title = "æ­£åœ¨åŠ è½½è„šæœ¬...", Icon = "downloading", ImageSource = "Material", Content = "å³å°†å¯åŠ¨: " .. scriptName })
    task.wait(1.5)
    Luna:Destroy()
    task.wait(0.1)
    loadstring(game:HttpGet(scriptUrl, true))()
end

