local function loadScript(scriptName, scriptUrl)
    Luna:Notification({ Title = "正在加载脚本...", Icon = "downloading", ImageSource = "Material", Content = "即将启动: " .. scriptName })
    task.wait(1.5)
    Luna:Destroy()
    task.wait(0.1)
    loadstring(game:HttpGet(scriptUrl, true))()
end

